{
  "name": "@dp/react-trix",
  "version": "0.4.2",
  "description": "React wrapper around Basecamp's Trix editor.",
  "main": "main.js",
  "scripts": {
    "build": "npm run build:lib && parcel build src/react-trix.tsx",
    "build:lib": "rm ./lib/* && tsc",
